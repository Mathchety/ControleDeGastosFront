{
  "expo": {
    "name": "ControleDeGastos",
    "slug": "controle-de-gastos-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.controle.gastos"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "permissions": [
        "CAMERA"
      ],
      "package": "com.controle.gastos",
      "softwareKeyboardLayoutMode": "pan",
      "navigationBar": {
        "visible": "immersive"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Permitir $(PRODUCT_NAME) acessar sua c√¢mera para escanear QR Codes de notas fiscais."
        }
      ]
    ]
  }
}
