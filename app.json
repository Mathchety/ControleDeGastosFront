{
  "expo": {
    "name": "Finansync",
    "slug": "finansync",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.finansync.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#667eea"
      },
      "permissions": [
        "CAMERA"
      ],
      "package": "com.finansync.app",
      "softwareKeyboardLayoutMode": "pan",
      "navigationBar": {
        "visible": "immersive"
      }
    },
    "web": {
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Permitir $(PRODUCT_NAME) acessar sua c√¢mera para escanear QR Codes de notas fiscais."
        }
      ]
    ]
  }
}
