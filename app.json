{
  "expo": {
    "name": "Finansync",
    "slug": "finansync",
    "version": "1.0.4",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.finansync.app"
    },
    "android": {
  "icon": "./assets/logo-finansync.png",
      "adaptiveIcon": {
        "backgroundColor": "#667eea"
      },
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA",
        "INTERNET"
      ],
      "package": "com.finansync.app",
  "softwareKeyboardLayoutMode": "pan"
    },
    "splash": {
      "image": "./assets/logo-finansync.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "web": {},
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir $(PRODUCT_NAME) acessar sua c√¢mera para escanear QR Codes de notas fiscais."
        }
      ],
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "5568015c-e9c5-40da-bf0c-dec44145d33d"
      }
    },
    "owner": "mathhety"
  }
}
